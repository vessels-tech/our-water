{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "babel": "babel ./src --out-dir lib --extensions '.ts,.tsx'",
    "watch": "tsc --watch",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "find_unit": "find . -name '*.unit.ts'",
    "unit": "find . -name '*.unit.ts' | xargs ts-mocha -R spec",
    "service_old": "mocha ./**/*.service.ts",
    "service": "find . -name '*.service.ts' | xargs ts-mocha -R spec"
  },
  "main": "lib/src/index.js",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/register": "^7.0.0",
    "@firebase/app-types": "0.x",
    "body-parser": "^1.18.2",
    "btoa": "^1.2.1",
    "cors": "^2.8.4",
    "dbf": "0.1.4",
    "express": "^4.16.3",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^0.4.0",
    "express-validation": "^1.0.2",
    "firebase-admin": "8.12.1",
    "firebase-app": "^0.0.1",
    "firebase-functions": "3.6.2",
    "google-auth-library": "^2.0.1",
    "gulp": "^3.9.1",
    "handlebars": "^4.0.12",
    "joi": "^13.2.0",
    "jsx-to-html": "^1.1.0",
    "jsx-xml": "^0.1.1",
    "jszip": "2.5.0",
    "mocha": "^5.1.1",
    "mock-cloud-firestore": "^0.9.2",
    "moment": "^2.22.1",
    "morgan": "^1.9.1",
    "morgan-body": "^2.4.5",
    "nodemailer": "^4.6.8",
    "ow_common": "^0.1.29",
